= simple_form_for @board, :html => { :class => 'form-horizontal' } do |f|
  = f.input :text
  = f.input :full_text
  = f.input :url
  = f.input :lat
  = f.input :long
  = f.button :submit, :class => 'btn-primary'
  = link_to t('.cancel', :default => t("helpers.links.cancel")), boards_path, :class => 'btn btn-default'
